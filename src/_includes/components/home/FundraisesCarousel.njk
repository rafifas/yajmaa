<div class="cards-carousel">
<div id="slideLeft">
<span class="icon is-medium">
  <i class="mdi mdi-24px mdi-chevron-left"></i>
</span>
</div>
<div id="slideRight">
<span class="icon is-medium">
  <i class="mdi mdi-24px mdi-chevron-right"></i>
</span>
</div>
    <div class="wrapper-abs" id="container">
        <div class="wrapper-flex">
            {% set fundraisesList = collections.fundraises %}
            {% for fundraise in fundraisesList | reverse %}
                <div class="wrapper-card" style="width: 21rem;">
                    {{ CardFra.cardFra(fundraise.data, fundraise.url) }}
                </div>
            {% endfor %}
            <div class="wrapper-card">
                <a href="{{ '/fundraises' }}">
                    <div class="see-all" style="">
                        <span class="icon is-large">
                            <i class="mdi mdi-36px mdi-arrow-right"></i>
                        </span>
                        <p class="has-text-bold has-text-weight-bold">Lihat Semua</p>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>